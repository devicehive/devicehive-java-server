<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" updateCheck="true"
         monitoring="autodetect" dynamicConfig="true">

    <diskStore path="/tmp/ehcache/data"/>

    <defaultCache maxElementsInMemory="10000" eternal="false"
                  timeToIdleSeconds="120" timeToLiveSeconds="120"
                  overflowToDisk="true" diskSpoolBufferSizeMB="30"
                  maxElementsOnDisk="10000000" diskExpiryThreadIntervalSeconds="120"
                  memoryStoreEvictionPolicy="LRU">
    </defaultCache>

    <cache name="deviceActivity" maxEntriesLocalHeap="10000" eternal="false"
           timeToIdleSeconds="400" timeToLiveSeconds="400">
    </cache>

    <cache name="org.hibernate.cache.StandardQueryCache"
            maxEntriesLocalHeap="5000" eternal="false"
            timeToLiveSeconds="120" timeToIdleSeconds="120"
            overflowToDisk="true" diskPersistent="true"
            diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.Device"
            maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
            timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
            diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.User"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.Equipment"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.DeviceClass"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.DeviceEquipment"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.Network"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.AccessKey"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.AccessKeyPermission"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.IdentityProvider"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.OAuthClient"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="com.devicehive.model.OAuthGrant"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>

    <cache name="queryCache"
           maxEntriesLocalHeap="1000" eternal="false" timeToIdleSeconds="120"
           timeToLiveSeconds="120" overflowToDisk="true" diskPersistent="true"
           diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LFU" >
    </cache>


    <cache name="org.hibernate.cache.internal.StandardQueryCache"
           maxEntriesLocalHeap="500" eternal="false" timeToLiveSeconds="120"
           overflowToDisk="true" diskPersistent="true" timeToIdleSeconds="120">
    </cache>

    <cache name="org.hibernate.cache.spi.UpdateTimestampsCache"
           maxEntriesLocalHeap="5000" eternal="true">
        <persistence strategy="localTempSwap" />
    </cache>

    <!--<cacheManagerPeerListenerFactory-->
            <!--class="org.terracotta.ehcachedx.monitor.probe.ProbePeerListenerFactory"-->
            <!--properties="monitorAddress=localhost, monitorPort=9889, memoryMeasurement=true" />-->
</ehcache>